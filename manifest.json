{
    "manifest_version": 2,
    "name": "BaitBuster",
    "version": "1.0",
    "description": "Alerts potential fake news, clickbait, and/or satire links",
    "content_scripts": [
        {
          "js": [
            "./jquery.min.js",
            "./main.js"
          ],
          
          "matches": [
            "<all_urls>",
            "https://www.google.com/*"
          ]
        }
    ],
    "browser_action": {
      "default_title": "Extension Name",
      "default_icon": "extensionicon.png",
      "default_popup": "popup.html"
    },
    "permissions": ["tabs"]
}